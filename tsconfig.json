{
  "compilerOptions": {
    "target": "ES2020",
    "types": ["cypress"],
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    },
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["./*.ts", "cypress/**/*.ts","src/**/*.ts", "src/**/*.d.ts", "cypress/support/index.ts"] 
} 
